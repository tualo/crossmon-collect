{
	"collectServer": "127.0.0.1",
	"collectPort": "7654",
	"collect": [
		{
			"module": "crossmon-cpu",
			"interval": "1000",
			"options": {
				"scale": 1,
				"cpu-all": true,
				"programs": [
					{
						"tag": "googlechrome",
						"contains": "Google Chrome"
					},
					{
						"tag": "mysqlserver",
						"contains": "mysqld"
					},
					{
						"tag": "apacheserver",
						"contains": "httpd"
					}
				]
			}
		}
	]
}